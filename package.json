{
  "name": "@globals/browser",
  "version": "1.0.0",
  "description": "Autogenerated JavaScript globals for all environments.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "collect": "tsx ./scripts/collect.ts && pnpm run format:write",
    "format:check": "dprint check",
    "format:write": "dprint fmt src/**/*.ts",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "keywords": [
    "browser",
    "globals",
    "confusing-browser-globals"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Rel1cx/browser-globals.git"
  },
  "bugs": {
    "url": "https://github.com/Rel1cx/browser-globals/issues"
  },
  "author": "Eva1ent",
  "license": "MIT",
  "packageManager": "pnpm@8.9.0",
  "devDependencies": {
    "@tsconfig/node20": "20.1.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20.8.6",
    "dprint": "0.41.0",
    "eslint": "8.51.0",
    "eslint-config-with-tsconfig": "2.7.3",
    "eslint-plugin-filenames-simple": "0.8.0",
    "playwright": "1.39.0",
    "publint": "0.2.4",
    "tsup": "7.2.0",
    "tsx": "3.13.0",
    "typescript": "5.2.2"
  }
}
