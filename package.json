{
    "name": "@globals/browser",
    "version": "1.0.1",
    "private": false,
    "description": "Auto-generated JavaScript globals for all environments.",
    "keywords": [
        "browser",
        "globals",
        "confusing-browser-globals"
    ],
    "bugs": {
        "url": "https://github.com/Rel1cx/browser-globals/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Rel1cx/browser-globals.git"
    },
    "license": "MIT",
    "author": "Eva1ent",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "collect": "tsx ./scripts/collect.ts && pnpm dprint fmt src/globals.ts",
        "format:check": "dprint check",
        "format:write": "dprint fmt",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "publish": "pnpm run format:write && pnpm run lint && pnpm run collect && pnpm run build && publint"
    },
    "devDependencies": {
        "@tsconfig/node20": "20.1.2",
        "@tsconfig/strictest": "2.0.2",
        "@types/node": "20.8.6",
        "dprint": "0.41.0",
        "eslint": "8.51.0",
        "eslint-config-with-tsconfig": "2.7.3",
        "eslint-plugin-filenames-simple": "0.8.0",
        "playwright": "1.39.0",
        "publint": "0.2.4",
        "tsup": "7.2.0",
        "tsx": "3.13.0",
        "typescript": "5.2.2"
    },
    "packageManager": "pnpm@8.9.0"
}
